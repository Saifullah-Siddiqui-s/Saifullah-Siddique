{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://saifullah-siddique-info.vercel.app/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(jpg|jpeg|png|gif|svg|ico|css|js)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
